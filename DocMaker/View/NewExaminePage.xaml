<Window x:Class="DocMaker.View.NewExaminePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DocMaker.View"
        mc:Ignorable="d"
        Title="Исследования" Height="600" Width="600" WindowStartupLocation="CenterOwner" Closed="Window_Closed">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="27*"/>
            <RowDefinition Height="113*"/>
        </Grid.RowDefinitions>

        <Grid Background="{DynamicResource FourthBrush}">
            <TextBlock Text="Добавить новое исследование" FontSize="18" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        </Grid>

        <ScrollViewer Grid.Row="1">
            <Viewbox>
                <Grid Height="180" Width="243">

                </Grid>
            </Viewbox>
        </ScrollViewer>

    </Grid>
</Window>
